<odoo>
    <record id="view_company_forminherit_ceylax_report" model="ir.ui.view">
        <field name="name">	res.company.form.inherit.ceylex</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <field name="phone" position="after">
                <field name="fax" />
            </field>
            <field name="favicon" position="after">
                <field name="invoice_footer" widget="image"/>
                <field name="company_short_code"/>
                <field name="company_short_seq" invisible="1"/>
            </field>
        </field>
    </record>

</odoo>