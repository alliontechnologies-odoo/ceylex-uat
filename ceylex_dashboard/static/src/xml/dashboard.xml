<?xml version="1.0" encoding="UTF-8" ?>
<templates id="ceylex_template" xml:space="preserve">
    <t t-name="CeyDashboard">
        <div class="cey_dashboard" >
            <div class="container-fluid o_ceylex_dashboard" >
            </div>
        </div>
    </t>
    <t t-name="MainDashboard">
        <section class="dashboard_main_section " id="main_section_login">
            <div class="row">
                <div class="col-sm-12 mb-4">
                    <div class="row mt16" >
                        <div class="col-12 col-sm-12 col-md-8">
                            <h2 class="section-header">Ceylex Dashboard</h2>
                        </div>
                        <div class="col-12 col-sm-12 col-md-4">

                            <div class="form-row">
                                <div class="col">
                                    <input type="date" id="inputStartDate" class="form-control" placeholder="Start Date" t-att-value="widget.start_date"/>
                                </div>
                                <div class="col">
                                    <input type="date" id="inputEndDate" class="form-control" placeholder="End Date" t-att-value="widget.end_date"/>
                                </div>
                                <div class="col">
                                    <input type="button" class="btn btn-primary" id="btnSearch" value="Search" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <hr/>
                </div>
            </div>
        </section>
    </t>

    <t t-name="EnergyDelivered">
        <section class="bg-white" >
            <div class="row " >

                <!--  __________________     Power Generation Widget  ___________________________    -->
                <div class="col-md-4  mt64 col-sm-12">
                        <div class="row">
                            <div class="col-md-12 my-3 bg-light">
                                <h3 class="text-center text-primary">Power Generations</h3>
                            </div>
                        </div>
                        <div class="crm_scroll_table my-3 bg-light">
                            <div class="items-table">
                                <div class="item-container">
                                    <t t-foreach="widget.power_generations_summery" t-as="power">

                                        <div class="row bg-info rounded-pill m-3" >
                                            <div class="col-md-5 m-auto text-center">
                                                <h4 >
                                                    <t t-esc="power['company']"/>
                                                    <t t-esc="widget.start_date"/>
                                                </h4>
                                            </div>
                                            <div class="col-md-7 ">
                                                <div class="item-content">
                                                    <ul>
                                                        <li>
                                                            <strong>Power Generated : </strong>
                                                            <span><t t-esc="power['power']"/></span>
                                                        </li>
                                                        <li>
                                                            <strong>Power Capacity : </strong>
                                                            <span><t t-esc="power['capacity']"/></span>
                                                        </li>
                                                        <li>
                                                            <strong>Plant Factor : </strong>
                                                            <span><t t-esc="power['plant_factor']"/></span>
                                                        </li>
                                                        <li>
                                                            <strong>Period : </strong>
                                                            <span><t t-esc="power['period']"/></span>
                                                        </li>
                                                    </ul>

                                                </div>
                                            </div>

                                        </div>
                                    </t>
                                </div>
                            </div>
                        </div>
                </div>

                <!--  __________________     Widget 7  ___________________ ________    -->
                <div class="col-md-8 col-sm-12  mt64" >
                    <div class="row">
                        <div class="col-md-12 my-3 bg-light">
                            <h3 class="text-center text-primary">Project-wise energy delivered to National Grid</h3>
                        </div>
                    </div>
                    <div class="row my-3 bg-light ">
                        <t t-foreach="widget.energy_delivered" t-as="energy">
                            <div class="col-4" >
                                <div>
                                    <div class="bg-secondary rounded-pill text-center p-2">
                                        <h4><t t-esc="energy['name']"/></h4>
                                        <strong><t t-esc="energy['total']"/></strong>
                                    </div>
                                </div>
                                <table class="table mt16">
                                    <thead >
                                        <tr class="table-info">
                                            <th>Month</th>
                                            <th>Submitted Date</th>
                                            <th>Value of Energy</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <t t-foreach="energy['record']" t-as="record">
                                            <tr>
                                                <td><t t-esc="record['month']"/></td>
                                                <td><t t-esc="record['date']"/></td>
                                                <td class="text-right"><t t-esc="record['value']"/></td>
                                            </tr>
                                        </t>
                                    </tbody>
                                </table>
                            </div>
                        </t>

                    </div>

                </div>

